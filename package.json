{
  "name": "Jinja2-Visualizer",
  "displayName": "Jinja2Visualizer",
  "description": "Visualize nested Jinja2 conditional blocks without modifying the file",
  "version": "0.0.2",
  "publisher": "mannubhai1",
  "repository": {
    "type": "git",
    "url": "https://github.com/mannubhai1/jinja2visualiser"
  },
  "categories": ["Visualization", "Other"],
  "keywords": ["jinja2", "template", "visualization", "if-else", "for"],
  "engines": {
    "vscode": "^1.85.0"
  },
  "activationEvents": [
    "onCommand:jinja2Visualizer.open"
  ],
  "main": "./out/extension.js",
  "contributes": {
    "commands": [
      {
        "command": "jinja2Visualizer.open",
        "title": "Jinja2: Open Visualizer"
      }
    ]
  },
  "scripts": {
    "compile": "tsc -p ./",
    "watch": "tsc -watch -p ./"
  },
  "devDependencies": {
    "@types/node": "^25.0.2",
    "@types/vscode": "^1.85.0",
    "typescript": "^5.3.0"
  }
}
